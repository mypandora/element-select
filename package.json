{
  "name": "@mypandora/element-select",
  "version": "0.0.2",
  "description": "一个基于 element-ui 的下拉框单选按钮组组件",
  "keywords": [
    "vue",
    "vue 2",
    "element-ui",
    "select",
    "radio"
  ],
  "homepage": "https://github.com/mypandora/element-select#readme",
  "bugs": {
    "url": "https://github.com/mypandora/element-select/issues"
  },
  "type": "module",
  "author": "mypandora",
  "files": [
    "dist"
  ],
  "main": "./dist/element-select.umd.cjs",
  "module": "./dist/element-select.js",
  "exports": {
    "import": "./dist/element-select.js",
    "require": "./dist/element-select.umd.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mypandora/element-select.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "commit": "commit",
    "dev": "vite",
    "build": "vite build",
    "lint-staged": "lint-staged",
    "release": "release-it --cli",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@release-it/conventional-changelog": "^8.0.2",
    "@vitejs/plugin-vue2": "^2.3.1",
    "element-ui": "^2.15.14",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "vite": "^5.4.8",
    "vue": "2.7.16"
  },
  "peerDependencies": {
    "element-ui": "^2.15.14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "packageManager": "pnpm@9.11.0",
  "volta": {
    "node": "20.17.0"
  }
}
